{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": [
      "esnext",
      "dom"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/pages/api/auth/[...nextauth].js"  ],
  "types": [
    "cypress",
    "cypress-axe",
    "node"
  ],
  "exclude": [
    "node_modules"
  ]

}
